.musiclist-c{
  
  display: flex;
  width: 100%;
  height: 100rpx;
  
  margin-top: 15rpx;

}
.musiclist-c-series{
  width: 50rpx;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  flex-grow: 1;
}
.musiclist-c-info{
  flex-grow: 2;
  width: 100%;
  height: 100rpx;
 
}
.info-titile{
  display: flex;
  
  width: 97%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
  text-indent: 10rpx;
   /* display: -webkit-box; */
  
}
.info-author{
  width: 90%;
  height: 40rpx;
  line-height: 40rpx;
  color:grey;
  font-size: 26rpx;
  text-indent: 10rpx;
  display: -webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:1;
  overflow: hidden;
  text-overflow: ellipsis;
}
.titile-name{
  display: -webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:1;
  overflow: hidden;
  text-overflow: ellipsis;
}
.titile-referral{
  flex-grow: 1;
  color:gray;
  display: -webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:1;
  overflow: hidden;
  text-overflow: ellipsis;
}
.actived{
  color:red;
}